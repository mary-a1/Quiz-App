<!DOCTYPE html>
<html lang="en">

<head>
  <title>Login</title>

  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link
    href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@1,800&family=Roboto:wght@500;900&display=swap"
    rel="stylesheet">

  <!-- App CSS -->
  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" />
  <link rel="stylesheet" href="/vendor/border-box.css" />
  <link rel="stylesheet" href="/styles/main.css" />
  <link rel="stylesheet" href="/styles/layout.css" />
  <link rel="stylesheet" href="/styles/header.css" />
  <link rel="stylesheet" href="/styles/results.css" />


  <!-- External Scripts -->
  <script src="/vendor/jquery-3.0.0.js"></script>

</head>

<body>
  <header>
    <%- include('./partials/header', {user}) %>
  </header>

  <% for (const result of results) { %>
    <p>
      <%= (result.user) ? 'Player: ' + result.user : 'Title: ' + result.title %>
    </p>
    <p>
      <%= (result.thumbnail_url) ? 'thumbnail_url: ' + result.thumbnail_url : '' %>
    </p>
    <p>
      Result: <%=result.score%>
    </p>
    <% } %>


</body>

</html>
